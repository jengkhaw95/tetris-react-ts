{
  "name": "tetris",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "ts-node -T server/index.ts",
    "nodemon": "nodemon server/index.ts",
    "typecheck": "tsc",
    "format-check": "prettier --check \"(src|utils|server)/**/*.{ts,tsx}\"",
    "format": "prettier --write \"(src|utils|server)/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.10.0",
    "ts-node": "^10.9.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  }
}
